<!-- app/views/quotes/new.html.erb -->
<div class="container">
  <h1>New Quote</h1>
  <%= form_with(model: @quote, local: true) do |form| %>
    <div class="form-group">
      <%= form.label :projects_name %>
      <%= form.text_field :projects_name, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= form.label :customer_name %>
      <%= form.text_field :customer_name, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= form.submit "Create Quote", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>